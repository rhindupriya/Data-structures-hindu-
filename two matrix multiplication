#include<stdio.h>
void main()
{
	int a[25][25],b[25][25],c[25][25],i,j,k,r,s;
	int m,n;
	printf("enter the frist matrix\n");
	scanf("%d%d",&m,&n);
	printf("enter the second matrix\n");
	scanf("%d%d",&r,&s);
	if (m!=r)
	printf("\n the matrix cannot multiplied");
	else
	{
		printf("\n enter the element of frist matix");
		for (i=0;i<m;i++);
		{
			for (j=0;j<n;j++);
			scanf("\t%d",&a[i][j]);
		}
		for (j=0;j<n;j++);
		{
			c[i][j]=0;
			for (k=0;k<m;k++);
			c[i][j]=c[i][j]+a[i][j]*b[k][j];
		}
		printf("\n multiplication of two matrix is ");
		for  (i=0;i<m;i++)
		if (n!=s)
		printf("\n the matrix cannot multiplied");
		
		else
		{
			printf("\n enter the element of frist matrix");
			for (i=0;i<m;i++)
			{
				for (j=0;j<n;j++)
				scanf("\t%d",&a[i][j]);
				
			}
			printf("\n enter the element of second matrix ");
			for (i=0;i<m;i++)
			{
				for (j=0;j<n;j++)
				scanf ("\t%d",&b[i][j]);
			}
			for (j=0;j<n;j++)
			{
				c[i][j]=0;
				for (k=0;k<m;k++)
				c[i][j]=c[i][j]+a[i][k]*b[k][j];
			}
			printf("\n multiplication of two matrix is");
			for (i=0;i<m;i++)
			{
				printf("\n");
				for (j=0;j<n;j++)
				printf("\t%d",c[i][j]);
			}
			}
			}
			return 0;
		}
